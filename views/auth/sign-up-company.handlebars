<!--begin::Main-->
<!--begin::Root-->
<div class="d-flex flex-column flex-root">
	<!--begin::Authentication - Sign-up -->
	<div
		class="d-flex flex-column flex-column-fluid bgi-position-y-bottom position-x-center bgi-no-repeat bgi-size-contain bgi-attachment-fixed">
		<!--begin::Content-->
		<div class="d-flex flex-center flex-column flex-column-fluid p-10 pb-lg-20">
			<!--begin::Logo-->
			<a href="/" class="mb-12">
				<img alt="Logo" src="/assets/media/logos/logo.png" class="h-75px" />
			</a>
			<!--end::Logo-->
			<!--begin::Wrapper-->
			<div class="w-lg-600px bg-body rounded shadow-sm p-10 p-lg-15 mx-auto">
				<div class="text-gray-400 fw-bold fs-4 mb-5 text-center">Insira abaixo os dados da empresa:</div>
				<!--begin::Form-->
				<form class="form w-100" novalidate="novalidate" id="company_form">
					<!--end::Heading-->
					<div id="error-area" class="text-center text-danger mb-4"></div>
					<!--begin::Separator-->
					<div class="d-flex align-items-center mb-10">
						<div class="border-bottom border-gray-300 mw-50 w-100"></div>
						<div class="border-bottom border-gray-300 mw-50 w-100"></div>
					</div>
					<!--end::Separator-->
					<!--begin::Input group-->
					<div class="row fv-row mb-7">
						<!--begin::Col-->
						<div class="col-xl-12">
							<label class="form-label fw-bolder text-dark fs-12 required">Razão Social</label>
							<input class="form-control form-control-lg form-control-solid" type="text" placeholder=""
								name="corporate_name" autocomplete="off"/>
						</div>
					</div>
					<div class="row fv-row mb-7">
						<div class="col-xl-6 mb-7">
							<label class="form-label fw-bolder text-dark fs-6 required">Nome Fantasia</label>
							<input class="form-control form-control-lg form-control-solid" type="text" placeholder=""
								name="fantasy_name" autocomplete="off" required/>

						</div>
						<div class="col-xl-6">
							<label class="form-label fw-bolder text-dark fs-6 required">CNPJ</label>
							<input class="form-control form-control-lg form-control-solid" type="text" placeholder=""
								name="cnpj" autocomplete="off" />

						</div>
					</div>


					<div class="row fv-row mb-7">
						<div class="col-xl-6 mb-7">
							<label class="form-label fw-bolder text-dark fs-12 required">CEP</label>
							<input class="form-control form-control-lg form-control-solid" type="text" placeholder=""
								name="zip_code" autocomplete="off" />
							<div id="error-zipcode-field" class="text-danger"></div>
						</div>
						<div class="col-xl-6">
							<label class="form-label fw-bolder text-dark fs-12">Estado</label>
							<input class="form-control form-control-lg form-control-solid" type="text" placeholder=""
								name="state" autocomplete="off" readonly/>
						</div>
					</div>
					<div class="row fv-row mb-7">
						<div class="col-xl-12 mb-7">
							<label class="form-label fw-bolder text-dark fs-12 ">Endereço</label>
							<input class="form-control form-control-lg form-control-solid" type="text" placeholder=""
								name="address" autocomplete="off" readonly/>
						</div>
						<div class="col-xl-8 mb-7">
							<label class="form-label fw-bolder text-dark fs-12 ">Complementos</label>
							<input class="form-control form-control-lg form-control-solid" type="text" placeholder=""
								name="complement" autocomplete="off" />
						</div>
						<div class="col-xl-4">
							<label class="form-label fw-bolder text-dark fs-6 required">Número</label>
							<input class="form-control form-control-lg form-control-solid" type="text" placeholder=""
								name="number" autocomplete="off" maxlength="6"/>
						</div>
					</div>
					<div class="row fv-row mb-7">
						<div class="col-xl-6 mb-7">
							<label class="form-label fw-bolder text-dark fs-6">Bairro</label>
							<input class="form-control form-control-lg form-control-solid" type="text" placeholder=""
								name="district" autocomplete="off" readonly/>
						</div>
						<div class="col-xl-6">
							<label class="form-label fw-bolder text-dark fs-6">Cidade</label>
							<input class="form-control form-control-lg form-control-solid" type="text" placeholder=""
								name="city" autocomplete="off" readonly/>
						</div>
					</div>
					<!--end::Input group-->
					<!--end::Input group-->
					<!--begin::Actions-->
					<div class="text-center">
						<button type="submit" id="button_sign_up_company"
							class="btn btn-lg btn-primary">
							<span class="indicator-label">Cadastrar</span>
							<span class="indicator-progress">
								<span class="spinner-border spinner-border-sm align-middle ms-2"></span>
						</button>
					</div>
					<!--end::Actions-->
				</form>
				<!--end::Form-->
			</div>
			<!--end::Wrapper-->
		</div>
		<!--end::Content-->
	</div>
	<!--end::Authentication - Sign-up-->
</div>
<!--end::Root-->
<!--end::Main-->

<div class="modal fade " id="alertModal" tabindex="-1" aria-labelledby="alertModalLabel" aria-hidden="true">
	<div class="modal-dialog modal-dialog-centered">
		<div class="modal-content">
			<div class="modal-body">
				<!-- Conteúdo do alerta -->
				<div class="alert alert-dismissible d-flex flex-center flex-column py-10 px-10 px-lg-20 mb-10">

					<!--begin::Wrapper-->
					<div class="text-center">
						<!--begin::Title-->
						<h1 class="fw-bolder-especial mb-5">BEM-VINDO!</h1>
						<!--end::Title-->

						<!--begin::Separator-->
						<div class="separator separator-dashed border-danger opacity-25 mb-5"></div>
						<!--end::Separator-->

						<!--begin::Content-->
						<div class="mb-12 text-dark fs-7 mt-9">
							Seu cadastro foi realizado com sucesso! Você será redirecionado para a área de login.</div>
						<!--end::Content-->

						<!--begin::Buttons-->
						<span class="svg-icon svg-icon-success svg-icon-5hx "><svg xmlns="http://www.w3.org/2000/svg"
								width="24" height="24" viewBox="0 0 24 24" fill="none">
